<f:form xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:f="http://schemas.nxkit.org/2014/xforms-layout" xmlns:xf="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:d="urn::data" id="form">
  <xf:model id="data">
    <xf:instance id="misc">
      <data xmlns="urn::data">
        <secondary>false</secondary>
        <secondary_relevant />
      </data>
    </xf:instance>
    <xf:bind ref="instance('misc')">
      <xf:bind ref="d:secondary" type="xsd:boolean" />
      <xf:bind ref="d:secondary_relevant" relevant="../d:secondary='true'" />
    </xf:bind>
    <xf:instance id="primaryperson" src="primaryperson.xml" />
    <xf:bind ref="instance('primaryperson')">
      <xf:bind ref="d:heightfeet" required="true()" type="xsd:integer" constraint=". &gt;= 2 and . &lt;= 10" />
      <xf:bind ref="d:heightinches" required="true()" type="xsd:integer" constraint=". &gt;= 0 and . &lt;= 11" />
      <xf:bind ref="d:weight" required="true()" type="xsd:integer" constraint=". &gt;= 20 and . &lt;= 1000" />
      <xf:bind ref="d:gender" required="true()" type="xsd:string" />
      <xf:bind ref="d:smokerstatus" calculate="instance('primaryquestions')/d:question[1]/d:answer='true' " />
      <xf:bind ref="d:dateofbirth" required="true()" type="xsd:date" constraint="instance('primaryelections')/d:age &gt;= 18 and instance('primaryelections')/d:age &lt;= 85" />
      <xf:bind ref="d:socialsecuritynumber" required="true()" constraint="matches(., '^(\d{3}-\d{2}-\d{4})|(\d{9})$')" />
      <xf:bind ref="d:telephone1" required="true()" constraint="string-length()&gt;=10" />
      <xf:bind ref="d:telephone2" required="false()" constraint=".='' or string-length()&gt;=10" />
    </xf:bind>
    <xf:instance id="secondaryperson" src="secondaryperson.xml" />
    <xf:bind ref="instance('secondaryperson')" id="secondary">
      <xf:bind ref="./*" relevant="instance('misc')/d:secondary='true'" />
      <xf:bind ref="d:heightfeet" required="true()" type="xsd:integer" />
      <xf:bind ref="d:heightinches" required="true()" type="xsd:integer" />
      <xf:bind ref="d:weight" required="true()" type="xsd:integer" />
      <xf:bind ref="d:dateofbirth" type="xsd:date" required="true()" />
      <xf:bind ref="d:socialsecuritynumber" required="true()" constraint="matches(., '^(\d{3}-\d{2}-\d{4})|(\d{9})$')" />
      <xf:bind ref="d:smokerstatus" calculate="instance('secondaryquestions')/d:question[1]/d:answer='true' " />
    </xf:bind>
    <xf:instance id="primaryelections" src="primaryelections.xml" />
    <xf:bind ref="instance('primaryelections')">
      <xf:bind ref="d:term-life/d:coverage-amount" type="xsd:integer" required="true()" constraint="number(.) &gt; 0" />
      <xf:bind ref="d:dateofbirth" calculate="instance('primaryperson')/d:dateofbirth" type="xsd:date" />
      <xf:bind ref="d:age" calculate="               if(  string(../d:dateofbirth)!='',  number(substring(xf:now(), 1, 4)) - number(substring(../d:dateofbirth, 1, 4)) +   if(    number(substring(xf:now(), 6, 2)) &lt; number(substring(../d:dateofbirth, 6, 2)),    -1,     if(     number(substring(xf:now(), 6, 2))    = number(substring(../d:dateofbirth, 6, 2)) and      number(substring(xf:now(), 9, 2)) &lt; number(substring(../d:dateofbirth, 9, 2)),     -1,      0    )   ),  '' )                     " />
    </xf:bind>
    <xf:instance id="secondaryelections" src="secondaryelections.xml" />
    <xf:bind ref="instance('secondaryelections')">
      <xf:bind ref="./*" relevant="instance('misc')/d:secondary='true'" />
      <xf:bind ref="d:term-life/d:elected" type="xsd:boolean" calculate="instance('misc')/d:secondary" />
      <xf:bind ref="d:term-life/d:coverage-amount" type="xsd:integer" required="true()" constraint="number(.) &gt; 0" />
      <xf:bind ref="d:dateofbirth" calculate="instance('secondaryperson')/d:dateofbirth" type="xsd:date" />
      <xf:bind ref="d:gender" calculate="instance('secondaryperson')/d:gender" />
      <xf:bind ref="d:smokerstatus" calculate="instance('secondaryperson')/d:smokerstatus" />
      <xf:bind ref="d:age" calculate="               if(  string(../d:dateofbirth)!='',  number(substring(xf:now(), 1, 4)) - number(substring(../d:dateofbirth, 1, 4)) +   if(    number(substring(xf:now(), 6, 2)) &lt; number(substring(../d:dateofbirth, 6, 2)),    -1,     if(     number(substring(xf:now(), 6, 2))    = number(substring(../d:dateofbirth, 6, 2)) and      number(substring(xf:now(), 9, 2)) &lt; number(substring(../d:dateofbirth, 9, 2)),     -1,      0    )   ),  '' )                     " />
    </xf:bind>
    <xf:instance id="primaryquestions" src="primaryquestions.xml" />
    <xf:bind ref="instance('primaryquestions')">
      <xf:bind ref="d:question/d:answer" required="true()" type="xsd:boolean" />
      <xf:bind ref="d:question/d:healthinfo" relevant="../d:answer='true' and ./@enable='true'">
        <xf:bind ref="./*" type="xsd:string" />
        <xf:bind ref="d:condition" required="true()" />
        <xf:bind ref="d:physicianname" required="true()" />
        <xf:bind ref="d:physicianaddress" required="true()" />
        <xf:bind ref="d:physicianphonenumber" required="true()" />
      </xf:bind>
    </xf:bind>
    <xf:instance id="secondaryquestions" src="secondaryquestions.xml" />
    <xf:bind ref="instance('secondaryquestions')">
      <xf:bind ref="./*" relevant="instance('misc')/d:secondary='true'" />
      <xf:bind ref="d:question/d:answer" required="true()" type="xsd:boolean" />
      <xf:bind ref="d:question/d:healthinfo" relevant="../d:answer='true' and ./@enable='true'">
        <xf:bind ref="./*" type="xsd:string" />
        <xf:bind ref="d:condition" required="true()" />
        <xf:bind ref="d:physicianname" required="true()" />
        <xf:bind ref="d:physicianaddress" required="true()" />
        <xf:bind ref="d:physicianphonenumber" required="true()" />
      </xf:bind>
    </xf:bind>
    <xf:instance id="relationships" src="relationships.xml" />
    <xf:instance id="beneficiaries" src="beneficiaries.xml" />
    <xf:bind ref="instance('beneficiaries')">
      <xf:bind ref="d:beneficiary">
        <xf:bind ref="d:firstname" type="xsd:string" required="true()" />
        <xf:bind ref="d:lastname" type="xsd:string" required="true()" />
        <xf:bind ref="d:percentage" type="xsd:integer" />
        <xf:bind ref="d:socialsecuritynumber" constraint="matches(., '^(\d{3}-\d{2}-\d{4})|(\d{9})$')" />
        <xf:bind ref="d:telephone1" required="false()" constraint="string-length()&gt;=10" />
        <xf:bind ref="d:telephone2" required="false()" constraint=".='' or string-length()&gt;=10" />
        <xf:bind ref="d:dateofbirth" required="true()" type="xsd:date" />
      </xf:bind>
    </xf:bind>
    <xf:submission id="primaryperson_submission" ref="instance('primaryperson')" method="put" action="primaryperson.xml">
      <xf:send ev:event="xforms-submit-done" submission="secondaryperson_submission" />
    </xf:submission>
    <xf:submission id="secondaryperson_submission" ref="instance('secondaryperson')" method="put" action="secondaryperson.xml">
      <xf:send ev:event="xforms-submit-done" submission="primaryelections_submission" />
    </xf:submission>
    <xf:submission id="primaryelections_submission" ref="instance('primaryelections')" method="put" action="primaryelections.xml">
      <xf:send ev:event="xforms-submit-done" submission="secondaryelections_submission" />
    </xf:submission>
    <xf:submission id="secondaryelections_submission" ref="instance('secondaryelections')" method="put" action="secondaryelections.xml">
      <xf:send ev:event="xforms-submit-done" submission="primaryquestions_submission" />
    </xf:submission>
    <xf:submission id="primaryquestions_submission" ref="instance('primaryquestions')" method="put" action="primaryquestions.xml">
      <xf:send ev:event="xforms-submit-done" submission="secondaryquestions_submission" />
    </xf:submission>
    <xf:submission id="secondaryquestions_submission" ref="instance('secondaryquestions')" method="put" action="secondaryquestions.xml">
      <xf:send ev:event="xforms-submit-done" submission="beneficiaries_submission" />
    </xf:submission>
    <xf:submission id="beneficiaries_submission" ref="instance('beneficiaries')" method="put" action="beneficiaries.xml">
      <xf:dispatch ev:event="xforms-submit-done" name="isis-form-next" targetid="form" bubbles="true" />
    </xf:submission>
  </xf:model>
  <f:section>
    <xf:label>Term Life Request For Coverage Form</xf:label>
    <f:segment>
      <f:segment>
        <xf:label>Protect What Matters Most Today</xf:label>
        <f:p>Ready to take the next step in securing your family's financial future? Alaska USA Federal Credit Union and The Prudential Insurance Company of America have made it easy by offering group term life insurance at competitive rates.</f:p>
      </f:segment>
      <f:segment>
        <xf:label>What Is Term Life Insurance?</xf:label>
        <f:p>Term life insurance is often the most affordable coverage because it offers protection for a specific number of years. You may want to purchase a term life insurance policy to:</f:p>
        <f:list>
          <f:item>Get maximum coverage at an affordable price</f:item>
          <f:item>Cover specific financial responsibilities like a mortgage or college expenses</f:item>
          <f:item>Supplement a permanent policy or work policy</f:item>
        </f:list>
      </f:segment>
      <f:segment>
        <xf:label>Simplified Issue Term Life Insurance</xf:label>
        <f:p>For coverage amounts up to $250,000, we offer simplified issue term life insurance, with a streamlined application and expedited approval process.  No physical examination or medical tests are required, in most cases—acceptance is based on your answers to four health questions along with other information you provide or give us permission to obtain.</f:p>
      </f:segment>
      <f:segment>
        <xf:label>Features and Benefits</xf:label>
        <f:list>
          <f:item>Up to $250,000 in life insurance coverage</f:item>
          <f:item>Valuable protection you can keep until age 75</f:item>
          <f:item>Supplement an existing policy or use as your main source of coverage</f:item>
          <f:item>No Medical Exam or Lab Work, in most cases</f:item>
        </f:list>
      </f:segment>
      <f:segment>
        <xf:label>Apply Now!</xf:label>
        <f:p>The online application process is quick and easy. After confirming your name, address and identifying details, you will be able to review the features and pricing of the plan, choose a coverage amount, and designate your beneficiaries.</f:p>
        <f:p>Before beginning, please take a few minutes to gather personal health information like your doctor's name and address, details of current or past medical conditions and personal information about those you wish to name as a beneficiary.</f:p>
      </f:segment>
    </f:segment>
  </f:section>
  <f:section>
    <xf:label>Primary Enrollment</xf:label>
    <xf:group ref="instance('primaryperson')">
      <xf:label>Additional Information About You</xf:label>
      <xf:help>Complete the form by providing your physical characterists, personal information, and contact information.</xf:help>
      <xf:input ref="d:dateofbirth">
        <xf:label>Date of Birth</xf:label>
        <xf:help>Provide your date of birth to continue.</xf:help>
        <xf:hint appearance="minimal">mm/dd/yyyy</xf:hint>
      </xf:input>
      <xf:input ref="d:socialsecuritynumber">
        <xf:label>Social Security Number</xf:label>
        <xf:hint appearance="minimal">___-__-____</xf:hint>
        <xf:alert>Your social security number is required to continue.</xf:alert>
      </xf:input>
      <xf:group appearance="minimal">
        <xf:label>Physical Characteristics</xf:label>
        <xf:range ref="d:heightfeet" start="2" end="15" step="1">
          <xf:label>Feet</xf:label>
        </xf:range>
        <xf:range ref="d:heightinches" start="0" end="11" step="1">
          <xf:label>Inches</xf:label>
        </xf:range>
        <xf:range ref="d:weight" start="50" end="500" step="5">
          <xf:label>Weight</xf:label>
        </xf:range>
        <xf:select1 ref="d:gender">
          <xf:label>Sex</xf:label>
          <xf:item>
            <xf:label>Male</xf:label>
            <xf:value>M</xf:value>
          </xf:item>
          <xf:item>
            <xf:label>Female</xf:label>
            <xf:value>F</xf:value>
          </xf:item>
        </xf:select1>
      </xf:group>
      <xf:group appearance="minimal">
        <xf:label>Contact Information</xf:label>
        <xf:input ref="d:telephone1">
          <xf:label>Home Phone</xf:label>
          <xf:hint appearance="minimal">1 (___) ___-____</xf:hint>
        </xf:input>
        <xf:input ref="d:telephone2">
          <xf:label>Work Phone</xf:label>
        </xf:input>
      </xf:group>
    </xf:group>
    <xf:group ref="instance('primaryelections')/d:term-life">
      <xf:label>Coverage Election</xf:label>
      <xf:hint>Choose the type of coverage and amounts for which you are applying.</xf:hint>
      <f:p>Choose the type of coverage and amounts for which you are applying.</f:p>
      <xf:select1 ref="d:coverage-amount" appearance="full">
        <xf:label>Select Your Coverage Amount</xf:label>
        <xf:item>
          <xf:label>$50,000</xf:label>
          <xf:value>50000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$75,000</xf:label>
          <xf:value>75000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$100,000</xf:label>
          <xf:value>100000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$150,000</xf:label>
          <xf:value>150000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$200,000</xf:label>
          <xf:value>200000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$250,000</xf:label>
          <xf:value>250000</xf:value>
        </xf:item>
      </xf:select1>
      <f:table>
        <xf:label>Monthly Rates per $10,000 of coverage</xf:label>
        <f:table-row>
          <f:table-cell>Age</f:table-cell>
          <f:table-cell>Non-Tobacco (Male)</f:table-cell>
          <f:table-cell>Tobacco (Female)</f:table-cell>
          <f:table-cell>Non-Tobacco (Female)</f:table-cell>
          <f:table-cell>Tobacco (Female)</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>Under 30</f:table-cell>
          <f:table-cell>$ 1.17</f:table-cell>
          <f:table-cell>$ 2.05</f:table-cell>
          <f:table-cell>$ 0.72</f:table-cell>
          <f:table-cell>$ 1.23</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>30-34</f:table-cell>
          <f:table-cell>$ 1.14</f:table-cell>
          <f:table-cell>$ 2.00</f:table-cell>
          <f:table-cell>$ 0.89</f:table-cell>
          <f:table-cell>$ 1.56</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>35-39</f:table-cell>
          <f:table-cell>$ 1.41</f:table-cell>
          <f:table-cell>$ 2.47</f:table-cell>
          <f:table-cell>$ 1.16</f:table-cell>
          <f:table-cell>$ 2.03</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>40-44</f:table-cell>
          <f:table-cell>$ 2.12</f:table-cell>
          <f:table-cell>$ 3.71</f:table-cell>
          <f:table-cell>$ 1.54</f:table-cell>
          <f:table-cell>$ 2.70</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>45-49</f:table-cell>
          <f:table-cell>$ 3.06</f:table-cell>
          <f:table-cell>$ 5.36</f:table-cell>
          <f:table-cell>$ 2.49</f:table-cell>
          <f:table-cell>$ 4.36</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>50-54</f:table-cell>
          <f:table-cell>$ 4.83</f:table-cell>
          <f:table-cell>$ 8.45</f:table-cell>
          <f:table-cell>$ 4.23</f:table-cell>
          <f:table-cell>$ 7.40</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>55-59</f:table-cell>
          <f:table-cell>$ 8.11</f:table-cell>
          <f:table-cell>$ 14.19</f:table-cell>
          <f:table-cell>$ 5.77</f:table-cell>
          <f:table-cell>$ 10.97</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>60-64</f:table-cell>
          <f:table-cell>$ 14.20</f:table-cell>
          <f:table-cell>$ 24.85</f:table-cell>
          <f:table-cell>$ 9.44</f:table-cell>
          <f:table-cell>$ 16.74</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>65-69</f:table-cell>
          <f:table-cell>$ 24.85</f:table-cell>
          <f:table-cell>$ 43.49</f:table-cell>
          <f:table-cell>$ 13.62</f:table-cell>
          <f:table-cell>$ 23.56</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>70-74*</f:table-cell>
          <f:table-cell>$ 39.80</f:table-cell>
          <f:table-cell>$ 62.78</f:table-cell>
          <f:table-cell>$ 19.28</f:table-cell>
          <f:table-cell>$ 29.93</f:table-cell>
        </f:table-row>
      </f:table>
      <f:p>The rates shown are available to eligible accountholders and spouses under age 70, subject to evidence of insurability acceptable to the insurer.
                Rates increase at age 30 and as you enter each 5-year age band thereafter. Coverage reduces at age 65 to 50% of the previous amount and again by 50% at age 70.
                Insurance coverage terminates at age 75. Male rates apply for both accountholders and spouses in MT.</f:p>
      <f:p>* Renewal rates only</f:p>
    </xf:group>
    <xf:group ref="instance('primaryquestions')">
      <xf:label>Health Questions</xf:label>
      <f:p>Please answer the following questions by checking “Yes” or “No.”</f:p>
      <f:p>If you answer “Yes” to any questions 2-4, you will be asked to provide full details just below the question.</f:p>
      <xf:repeat ref="d:question">
        <xf:group>
          <xf:select1 ref="d:answer" appearance="full" incremental="true">
            <xf:label ref="../d:text" />
            <xf:item>
              <xf:label>provide your answer...</xf:label>
              <xf:value />
            </xf:item>
            <xf:item>
              <xf:label>No</xf:label>
              <xf:value>false</xf:value>
            </xf:item>
            <xf:item>
              <xf:label>Yes</xf:label>
              <xf:value>true</xf:value>
            </xf:item>
          </xf:select1>
          <xf:group ref="d:healthinfo" appearance="full">
            <xf:input ref="d:condition">
              <xf:label>Condition</xf:label>
            </xf:input>
            <xf:input ref="d:dates">
              <xf:label>Dates</xf:label>
            </xf:input>
            <xf:input ref="d:currentstatus">
              <xf:label>Current Status</xf:label>
            </xf:input>
            <xf:input ref="d:physicianname">
              <xf:label>Physician Name</xf:label>
            </xf:input>
            <xf:input ref="d:physicianaddress">
              <xf:label>Physician Address</xf:label>
            </xf:input>
            <xf:input ref="d:physicianphonenumber">
              <xf:label>Physician Phone Number</xf:label>
            </xf:input>
          </xf:group>
        </xf:group>
      </xf:repeat>
    </xf:group>
  </f:section>
  <f:section>
    <xf:label appearance="compact">Spousal Enrollment</xf:label>
    <xf:input ref="instance('misc')/d:secondary">
      <xf:label>Yes, I'd like to take advantage of this offer for my spouse or domestic partner!</xf:label>
    </xf:input>
    <f:p>Please complete the following sections with information about your spouse or partner.</f:p>
    <xf:group ref="instance('misc')/d:secondary_relevant">
      <xf:label>Spouse Information</xf:label>
      <xf:group context="instance('secondaryperson')">
        <xf:label>Name</xf:label>
        <xf:input ref="d:firstname">
          <xf:label>First</xf:label>
        </xf:input>
        <xf:input ref="d:middlename">
          <xf:label>Middle</xf:label>
        </xf:input>
        <xf:input ref="d:lastname">
          <xf:label>Last</xf:label>
        </xf:input>
      </xf:group>
      <xf:group context="instance('secondaryperson')">
        <xf:label>Physical Characteristics</xf:label>
        <xf:range ref="d:heightfeet" start="2" end="15" step="1">
          <xf:label>Feet</xf:label>
        </xf:range>
        <xf:range ref="d:heightinches" start="0" end="11" step="1">
          <xf:label>Inches</xf:label>
        </xf:range>
        <xf:range ref="d:weight" start="50" end="500" step="5">
          <xf:label>Weight</xf:label>
        </xf:range>
        <xf:select1 context="instance('secondaryperson')" ref="d:gender">
          <xf:label>Sex</xf:label>
          <xf:item>
            <xf:label>Male</xf:label>
            <xf:value>M</xf:value>
          </xf:item>
          <xf:item>
            <xf:label>Female</xf:label>
            <xf:value>F</xf:value>
          </xf:item>
        </xf:select1>
      </xf:group>
      <xf:input context="instance('secondaryperson')" ref="d:dateofbirth">
        <xf:label>Date of Birth</xf:label>
      </xf:input>
      <xf:input context="instance('secondaryperson')" ref="d:socialsecuritynumber">
        <xf:label>Social Security Number</xf:label>
      </xf:input>
    </xf:group>
    <xf:group ref="instance('misc')/d:secondary_relevant">
      <xf:label>Spouse Coverage Election</xf:label>
      <xf:hint>Choose the type of coverage and amounts for which you are applying.</xf:hint>
      <f:p>Spouse/Partner coverage amount cannot be greater than that of the primary insured.</f:p>
      <xf:select1 context="instance('secondaryelections')/d:term-life" ref="d:coverage-amount">
        <xf:label>Coverage Amount</xf:label>
        <xf:item>
          <xf:label>$50,000</xf:label>
          <xf:value>50000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$75,000</xf:label>
          <xf:value>75000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$100,000</xf:label>
          <xf:value>100000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$150,000</xf:label>
          <xf:value>150000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$200,000</xf:label>
          <xf:value>200000</xf:value>
        </xf:item>
        <xf:item>
          <xf:label>$250,000</xf:label>
          <xf:value>250000</xf:value>
        </xf:item>
      </xf:select1>
      <f:table>
        <f:table-row>
          <f:table-cell>Age</f:table-cell>
          <f:table-cell>Non-Tobacco</f:table-cell>
          <f:table-cell>Tobacco</f:table-cell>
          <f:table-cell>Non-Tobacco</f:table-cell>
          <f:table-cell>Tobacco</f:table-cell>
        </f:table-row>
        <f:table-row>
          <f:table-cell>Under 30</f:table-cell>
          <f:table-cell>$ 1.17</f:table-cell>
          <f:table-cell>$ 2.05</f:table-cell>
          <f:table-cell>$ 0.72</f:table-cell>
          <f:table-cell>$ 1.23</f:table-cell>
