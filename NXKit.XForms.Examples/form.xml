                    <f:table-cell>Non-Tobacco</f:table-cell>
                    <f:table-cell>Tobacco</f:table-cell>
                    <f:table-cell>Non-Tobacco</f:table-cell>
                    <f:table-cell>Tobacco</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>Under 30</f:table-cell>
