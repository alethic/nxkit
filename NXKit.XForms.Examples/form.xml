                    </f:table-cell>
                    <f:table-cell>
                        $ 0.72
                        <xf:setvalue ev:event="DOMActivate"
                                     context="instance('misc')/d:rate"
                                     ref=".">0.72</xf:setvalue>
                    </f:table-cell>
                    <f:table-cell>
                        $ 1.23
                        <xf:setvalue ev:event="DOMActivate"
                                     context="instance('misc')/d:rate"
                                     ref=".">1.23</xf:setvalue>
                    </f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>30-34</f:table-cell>
                    <f:table-cell>$ 1.14</f:table-cell>
                    <f:table-cell>$ 2.00</f:table-cell>
                    <f:table-cell>$ 0.89</f:table-cell>
                    <f:table-cell>$ 1.56</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>35-39</f:table-cell>
                    <f:table-cell>$ 1.41</f:table-cell>
                    <f:table-cell>$ 2.47</f:table-cell>
                    <f:table-cell>$ 1.16</f:table-cell>
                    <f:table-cell>$ 2.03</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>40-44</f:table-cell>
                    <f:table-cell>$ 2.12</f:table-cell>
                    <f:table-cell>$ 3.71</f:table-cell>
                    <f:table-cell>$ 1.54</f:table-cell>
                    <f:table-cell>$ 2.70</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>45-49</f:table-cell>
                    <f:table-cell>$ 3.06</f:table-cell>
                    <f:table-cell>$ 5.36</f:table-cell>
                    <f:table-cell>$ 2.49</f:table-cell>
                    <f:table-cell>$ 4.36</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>50-54</f:table-cell>
                    <f:table-cell>$ 4.83</f:table-cell>
                    <f:table-cell>$ 8.45</f:table-cell>
                    <f:table-cell>$ 4.23</f:table-cell>
                    <f:table-cell>$ 7.40</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>55-59</f:table-cell>
                    <f:table-cell>$ 8.11</f:table-cell>
                    <f:table-cell>$ 14.19</f:table-cell>
                    <f:table-cell>$ 5.77</f:table-cell>
                    <f:table-cell>$ 10.97</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>60-64</f:table-cell>
                    <f:table-cell>$ 14.20</f:table-cell>
                    <f:table-cell>$ 24.85</f:table-cell>
                    <f:table-cell>$ 9.44</f:table-cell>
                    <f:table-cell>$ 16.74</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>65-69</f:table-cell>
                    <f:table-cell>$ 24.85</f:table-cell>
                    <f:table-cell>$ 43.49</f:table-cell>
                    <f:table-cell>$ 13.62</f:table-cell>
                    <f:table-cell>$ 23.56</f:table-cell>
                </f:table-row>
                <f:table-row>
                    <f:table-cell>70-74*</f:table-cell>
                    <f:table-cell>$ 39.80</f:table-cell>
                    <f:table-cell>$ 62.78</f:table-cell>
                    <f:table-cell>$ 19.28</f:table-cell>
                    <f:table-cell>$ 29.93</f:table-cell>
                </f:table-row>
            </f:table>
            <f:p>* Renewal rates only</f:p>
            <f:p>
            <xf:select1 ref="d:coverage-amount"
                        appearance="compact">
                <xf:label>Coverage Amount</xf:label>
                <xf:item>
                    <xf:label>$75,000</xf:label>
                    <xf:value>75000</xf:value>
                </xf:item>
                <xf:item>
                    <xf:label>$100,000</xf:label>
                    <xf:value>100000</xf:value>
                </xf:item>
                <xf:item>
                    <xf:label>$150,000</xf:label>
                    <xf:value>150000</xf:value>
                </xf:item>
            </xf:select1> X $<xf:output value="instance('misc')/d:rate"/> ÷ $10,000 = $<xf:output value="format-number(instance('misc')/d:premium, '#,###.00')" /> / month
            </f:p>
        </xf:group>
        <xf:group ref="instance('primaryquestions')">
            <xf:label>Health Questions</xf:label>
            <f:p>Please answer the following questions by checking “Yes” or “No.”</f:p>
            <f:p>If you answer “Yes” to any questions 2-4, you will be asked to provide full details just below the question.</f:p>
            <xf:repeat ref="d:question">
                <xf:group>
                    <xf:select1 ref="d:answer"
                                appearance="full"
                                incremental="true">
                        <xf:label ref="../d:text" />
                        <xf:item>
                            <xf:label>No</xf:label>
                            <xf:value>false</xf:value>
                        </xf:item>
                        <xf:item>
                            <xf:label>Yes</xf:label>
                            <xf:value>true</xf:value>
                        </xf:item>
                    </xf:select1>
                    <xf:group ref="d:healthinfo"
                              appearance="full">
                        <xf:input ref="d:condition">
                            <xf:label>Condition</xf:label>
                        </xf:input>
                        <xf:input ref="d:dates">
                            <xf:label>Dates</xf:label>
                        </xf:input>
                        <xf:input ref="d:currentstatus">
                            <xf:label>Current Status</xf:label>
                        </xf:input>
                        <xf:input ref="d:physicianname">
                            <xf:label>Physician Name</xf:label>
                        </xf:input>
                        <xf:input ref="d:physicianaddress">
                            <xf:label>Physician Address</xf:label>
                        </xf:input>
                        <xf:input ref="d:physicianphonenumber">
                            <xf:label>Physician Phone Number</xf:label>
                        </xf:input>
                    </xf:group>
                </xf:group>
            </xf:repeat>
        </xf:group>
