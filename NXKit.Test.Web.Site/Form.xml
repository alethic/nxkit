            <f:table-cell>
              <xf:select1 ref="elected" appearance="full">
                <xf:item>
                  <xf:label>Decline</xf:label>
                  <xf:value>false</xf:value>
                </xf:item>
              </xf:select1>
            </f:table-cell>
            <f:table-cell />
          </f:table-row>
        </f:table>
      </f:section>
