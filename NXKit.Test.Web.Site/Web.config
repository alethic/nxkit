<?xml version="1.0"?>

<configuration>

    <system.web>

        <httpRuntime
            requestValidationMode="2.0" />

        <pages
            validateRequest="false" />

        <compilation
            debug="true"
            targetFramework="4.5" />

        <customErrors
            mode="Off" />

    </system.web>

    <system.webServer>
        <staticContent>
            <mimeMap
                fileExtension=".less"
                mimeType="text/css" />
        </staticContent >
    </system.webServer>

</configuration>