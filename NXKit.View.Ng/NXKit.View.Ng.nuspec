        <version>$NuGetVersion$</version>
        <title>NXKit View for AngularJS</title>
        <authors>Jerome Haltom</authors>
        <owners>Jerome Haltom</owners>
        <licenseUrl>http://www.gnu.org/licenses/gpl.txt</licenseUrl>
        <projectUrl>https://github.com/nxkit</projectUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>
            NXKit view directive for AngularJS.
        </description>
        <releaseNotes />
        <copyright>Copyright © 2014 Jerome Haltom</copyright>
        <tags>xml xforms</tags>
        <dependencies>
            <dependency
                id="NXKit.View.Js"
                version="[$NuGetVersion$]"/>
            <dependency
                id="NXKit.View.Server"
                version="[$NuGetVersion$]"/>
        </dependencies>
